class Solution {
    public int numSubarrayProductLessThanK(int[] nums, int k) {
        int prod=1;
        int ws=0;
        int we=0;
        int c=0;
        if(k<=1){
            return 0;
        }
        while(we<nums.length){
            prod*=nums[we];
            while(prod>=k){
                prod/=nums[ws];
                ws++;
            }
            c+=we-ws+1;
            we++;
        }
        return c;
    }
}
